{
  "name": "rusty-store-kv",
  "version": "0.0.1",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "rusty-store-kv",
    "triples": {
      "defaults": false,
      "additional": [
        "x86_64-pc-windows-msvc",
        "x86_64-unknown-linux-gnu",
        "x86_64-unknown-linux-musl",
        "universal-apple-darwin"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.18.4",
    "vitest": "^2.0.5"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "vitest",
    "universal": "napi universal",
    "version": "napi version"
  },
  "optionalDependencies": {
    "rusty-store-kv-win32-x64-msvc": "0.0.1",
    "rusty-store-kv-linux-x64-gnu": "0.0.1",
    "rusty-store-kv-linux-x64-musl": "0.0.1",
    "rusty-store-kv-darwin-universal": "0.0.1"
  }
}